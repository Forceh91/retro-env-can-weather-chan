    "@types/react-dom": "^18.2.13",
    "@types/timezoned-date": "^3.0.0",
    "@types/uuid4": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "events": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mockdate": "^3.0.5",
