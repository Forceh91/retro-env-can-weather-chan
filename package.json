    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "ec-weather-js": "^1.2.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "point-in-polygon": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.2",
    "sass": "^1.69.3",
    "uuid4": "^2.0.3",
    "xml-js": "^1.6.11"
